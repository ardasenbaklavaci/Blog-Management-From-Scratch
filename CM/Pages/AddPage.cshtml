@page
@model CM.Pages.AddPageModel
@{
}
<form method="post" enctype="multipart/form-data">

    <table class="table table-warning">
        <tr>
            <td>ID: </td>
            <td><input type="text" class="form-control" name="idForm" /></td>
        </tr>
        <tr>
            <td>Name: </td>
            <td><input type="text" class="form-control" name="nameForm" /></td>
        </tr>
        <tr>
            <td>Parent: </td>
            <td><input type="text" class="form-control" name="parentForm" /></td>
        </tr>
        <tr>
            <td>HTML:</td>
            <td><textarea id="HTML" name="HTMLForm" class="form-control" rows="4"></textarea></td>
        </tr>

        <tr>
            <td>Title:</td>
            <td><textarea id="HTML" name="titleForm"  class="form-control" rows="4"></textarea></td>
        </tr>

        <tr>
            <td>Filename:</td>
            <td><textarea id="HTML" name="filenameForm"  class="form-control" rows="4"></textarea></td>
        </tr>

        <tr>
            <td>Has Content</td>
            <td><input type="checkbox" name="checkbox1" id=" checkbox1" runat="server" checked="@Model.chc" /></td>         
        </tr>
    </table>

    <button type="submit">Add</button>

</form>